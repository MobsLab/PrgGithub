function check_tracking(fname,percent)

%Allows to get position from frames and check tracking
cd(fname)
direct=dir;
framenum=size(direct,1)-3;
nx = 32;
ny = 24;
% try
% clear position vitesse
% end
figure(32)
% clear position vitesse
start = 1;
for i=start:5:framenum*percent
        eval(strcat('load(''',direct(2+i).name,''')'))
    if i==1
        t0=datas.time;
    end
%     position(3,i-start+1)=datas.time;
% position(3,i)=etime(datas.time,t0);
    position(1,i-start+1)=datas.centroid(1);
    position(2,i-start+1)=datas.centroid(2);
    imagesc(datas.image)
    axis image
    colormap gray
    hold on
    try
    plot(position(1,i-start+1),position(2,i-start+1), '-m+')
%     title(num2str(datas.time))
   pause(0.02)
   clf
end
etime(datas.time,t0)
cd ..

end