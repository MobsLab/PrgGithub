nametoerase='OF-VEH1-12-refMouseTracking-offline';
replaceby='BULB-Mouse-A1-20052014-OpenFieldVEH1-TrackingOffline';


lis=dir;
for i=1:length(lis)
    temp=lis(i).name;
    if ~isempty(strfind(temp,nametoerase))
        indtemp=strfind(temp,nametoerase);
        newname=[temp(1:indtemp-1),replaceby,temp(indtemp+length(nametoerase):end)];
        disp(['rename ',temp])
        disp(['-> ',newname])
        movefile(temp,newname);
        
    end
end
        
    