function DeltaEvolution(Dpfc,Dpac)


rg=Range(Dpfc,'s');
rg2=Range(Dpac,'s');

id=find(diff(rg)<0.7);
id2=find(diff(rg2)<0.7);

figure('color',[1 1 1]), 
subplot(2,4,1:2), plot(rg(2:end),diff(rg),'k'), ylabel('PFCx')
hold on, plot(rg(2:end),SmoothDec(diff(rg),3),'r','linewidth',2)
hold on, plot(rg(id),0.5*ones(length(id),1),'bo','markerfacecolor','b')
ylim([0 10])

subplot(2,4,[3,7]), hold on, plot(rg(id),'k','linewidth',2)

%[h1a,b1a]=hist(diff(rg),[0:0.01:2]);h1a(end)=0;
[h1b,b1b]=hist(diff(rg(1:floor(length(rg)/3))),[0:0.01:2]);h1b(end)=0;
[h1c,b1c]=hist(diff(rg(2*floor(length(rg)/3):end)),[0:0.01:2]);h1c(end)=0;
[C1b,B1b]=CrossCorr(rg(1:floor(length(rg)/3)),rg(1:floor(length(rg)/3)),10,500);C1b(B1b==0)=0;
[C1c,B1c]=CrossCorr(rg(2*floor(length(rg)/3):end),rg(2*floor(length(rg)/3):end),10,500);C1c(B1c==0)=0;

subplot(2,4,4), hold on
plot(b1b,h1b,'k')
plot(b1c,h1c,'r')
plot(B1b,C1b,'b')
plot(B1c,C1c,'m')
xlim([0 1.9])



subplot(2,5,5:6), plot(rg2(2:end),diff(rg2),'k'), ylabel('PaCx')
hold on, plot(rg2(2:end),SmoothDec(diff(rg2),3),'r','linewidth',2)
hold on, plot(rg2(id2),0.5*ones(length(id2),1),'bo','markerfacecolor','b')
ylim([0 10])

subplot(2,5,[3,7]), plot(rg2(id2),'r','linewidth',2)

% [h2a,b2a]=hist(diff(rg2),[0:0.01:2]);h2a(end)=0;
[h2b,b2b]=hist(diff(rg2(1:floor(length(rg)/3))),[0:0.01:2]);h2b(end)=0;
[h2c,b2c]=hist(diff(rg2(2*floor(length(rg)/3):end)),[0:0.01:2]);h2c(end)=0;
[C2b,B2b]=CrossCorr(rg2(1:floor(length(rg2)/3)),rg2(1:floor(length(rg2)/3)),10,500);C1b(B1b==0)=0;
[C2c,B2c]=CrossCorr(rg2(2*floor(length(rg2)/3):end),rg2(2*floor(length(rg2)/3):end),10,500);C1c(B1c==0)=0;

subplot(2,5,8), hold on
plot(b2a,h2a,'k')
plot(b2b,h2b,'b')
plot(b2c,h2c,'r')
xlim([0 1.9])
